{
  "name": "video-thumbnail-wasm",
  "version": "0.1.0",
  "description": "Video thumbnail generation with Rust and WebAssembly",
  "main": "pkg/video_thumbnail_wasm.js",
  "types": "pkg/video_thumbnail_wasm.d.ts",
  "files": [
    "pkg/**/*"
  ],
  "scripts": {
    "build": "wasm-pack build --target web --out-dir pkg --out-name index",
    "build:release": "wasm-pack build --target web --out-dir pkg --out-name index --release",
    "build:node": "wasm-pack build --target nodejs --out-dir pkg-node --out-name index",
    "build:bundler": "wasm-pack build --target bundler --out-dir pkg-bundler --out-name index"
  },
  "devDependencies": {
    "wasm-pack": "^0.12.1"
  },
  "keywords": [
    "video",
    "thumbnail",
    "webassembly",
    "rust",
    "ffmpeg"
  ],
  "license": "MIT"
}